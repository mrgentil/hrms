  @@index([status], map: "feedback_status")
}

model perf_improvement_plan {
  id          Int                  @id @default(autoincrement())
  review_id   Int?                 @unique
  employee_id Int
  manager_id  Int
  title       String               @db.VarChar(255)
  reason      String?              @db.Text
  start_date  DateTime             @db.Date
  end_date    DateTime             @db.Date
  status      perf_plan_status     @default(DRAFT)
  created_at  DateTime             @default(now()) @db.DateTime(0)
  updated_at  DateTime             @updatedAt @db.DateTime(0)
  actions     improvement_action[]
  employee    User? @relation("improvement_employee", fields: [employee_id], references: [id], onDelete: Cascade)
  manager     User? @relation("improvement_manager", fields: [manager_id], references: [id])
  review      performance_review?  @relation(fields: [review_id], references: [id])

  @@index([employee_id], map: "improvement_employee")
  @@index([manager_id], map: "improvement_manager")
  @@index([status], map: "improvement_status")
}

model improvement_action {
  id             Int                   @id @default(autoincrement())
  plan_id        Int
  title          String                @db.VarChar(255)
  description    String?               @db.Text
  due_date       DateTime?             @db.Date
  completed_at   DateTime?             @db.DateTime(0)
  status         perf_action_status    @default(PENDING)
  employee_notes String?               @db.Text
  manager_notes  String?               @db.Text
  created_at     DateTime              @default(now()) @db.DateTime(0)
  updated_at     DateTime              @updatedAt @db.DateTime(0)
  plan           perf_improvement_plan @relation(fields: [plan_id], references: [id], onDelete: Cascade)

  @@index([plan_id], map: "action_plan")
}

model recognition {
  id           Int              @id @default(autoincrement())
  from_user_id Int
  to_user_id   Int
  type         recognition_type @default(KUDOS)
  badge        String?          @db.VarChar(50)
  message      String           @db.Text
  is_public    Boolean          @default(true)
  created_at   DateTime         @default(now()) @db.DateTime(0)
  from_user    User? @relation("recognition_from", fields: [from_user_id], references: [id], onDelete: Cascade)
  to_user      User? @relation("recognition_to", fields: [to_user_id], references: [id], onDelete: Cascade)

  @@index([from_user_id], map: "recognition_from")
  @@index([to_user_id], map: "recognition_to")
  @@index([created_at], map: "recognition_date")
}

